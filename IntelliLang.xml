<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="JQuery-CSS" injector-id="xml">
      <display-name>div/@onmouseover</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("onmouseover")).withParent(xmlTag().withLocalName(string().equalTo("div")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
    <injection language="CSS" injector-id="xml">
      <display-name>head</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("head")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
    <injection language="HTML" injector-id="xml">
      <display-name>html</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("html")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
  </component>
</application>