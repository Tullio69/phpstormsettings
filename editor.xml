<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Source Code Pro" />
    <option name="LINE_SPACING" value="1.1" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="IS_VIRTUAL_SPACE" value="true" />
    <option name="IS_CARET_INSIDE_TABS" value="true" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="IS_ALL_SOFTWRAPS_SHOWN" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern case-sensitive="true" pattern="\btodo\b.*" />
    <pattern case-sensitive="true" pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bnote\b.*">
      <option name="FOREGROUND" value="a8c023" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="3c3f41" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
      <option name="EFFECT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bdebug\b.*">
      <option name="FOREGROUND" value="c01f7a" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <filter name="Debug">
      <pattern index="3" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>