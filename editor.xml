<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Consolas" />
    <option name="LINE_SPACING" value="1.2" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="IS_CARET_INSIDE_TABS" value="true" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="IS_MOUSE_CLICK_SELECTION_HONORS_CAMEL_WORDS" value="false" />
  </component>
  <component name="JSEditorOptions">
    <option name="START_STRING_INTERPOLATION_ON_TYPING" value="true" />
  </component>
  <component name="LiveEditOptions" chromeUpdateApp="true">
    <option name="elementHighlighting" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern case-sensitive="true" pattern="\btodo\b.*" />
    <pattern case-sensitive="true" pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bnote\b.*">
      <option name="FOREGROUND" value="a8c023" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="3c3f41" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
      <option name="EFFECT_TYPE" value="2" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bdebug\b.*">
      <option name="FOREGROUND" value="c01f7a" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bnotinprod\b.*">
      <option name="FOREGROUND" value="e4e3d6" />
      <option name="BACKGROUND" value="d1243b" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="32424a" />
      <option name="EFFECT_TYPE" value="4" />
    </pattern>
    <filter name="allin">
      <pattern index="4" />
      <pattern index="2" />
      <pattern index="0" />
      <pattern index="1" />
      <pattern index="3" />
    </filter>
    <filter name="Block For Prod">
      <pattern index="4" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>